{
  "name": "graphql-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "start:dev": "NODE_TLS_REJECT_UNAUTHORIZED='0' DEBUG=express-session nodemon --watch src -e ts --exec \"yarn start\"",
    "start:docker": "./docker-startup.sh && yarn start:dev",
    "generate-env": "ts-node generate-env.ts env=live interactive",
    "generate-env:sample": "ts-node generate-env.ts",
    "test": "mocha -r ts-node/register 'tests/**/*.ts'"
  },
  "dependencies": {
    "@typegoose/typegoose": "^9.12.1",
    "@types/graphql-upload": "^8.0.12",
    "apollo-server": "^3.10.3",
    "apollo-server-express": "^3.10.3",
    "argon2": "^0.30.1",
    "axios": "^1.1.3",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "graphql": "^15.3.0",
    "graphql-redis-subscriptions": "^2.5.0",
    "graphql-type-json": "^0.3.2",
    "graphql-upload": "12",
    "ioredis": "^5.2.3",
    "mongoose": "~6.6.0",
    "nodemailer": "^6.8.0",
    "subscriptions-transport-ws": "^0.11.0",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.19",
    "@types/cookie-parser": "^1.4.3",
    "@types/express-session": "^1.17.5",
    "@types/node": "^18.11.7",
    "@types/nodemailer": "^6.4.6",
    "@types/uuid": "^8.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
